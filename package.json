{
 "name": "react-trackhub",
 "version": "0.0.1",
 "description": "A react frontend for making custom Trackhub files",
 "author": "Denise Mauldin <denisemauldin> and Kristopher Skelton <kmskelton>",
 "license": "MIT",
 "scripts": {
  "build-biodalliance": "npx webpack ./src/biodalliance/index.js -o ./dist/assets/dalliance.js -p --display errors-only",
  "build-dev": "npm run clean-dist && npm run build-dev-client && npm run build-dev-server && npm run build-biodalliance",
  "build-dev-client": "npx webpack --progress --colors --config webpack.client.dev.config.js",
  "build-dev-server": "npx webpack --progress --colors --config webpack.server.dev.config.js",
  "build-prod": "npm run clean-dist && npm run build-prod-client && npm run build-prod-server && npm run build-biodalliance",
  "build-prod-client": "npx webpack --progress --colors --config webpack.client.prod.config.js",
  "build-prod-server": "npx webpack --progress --colors --config webpack.server.prod.config.js",
  "clean-dist": "npx rimraf ./dist/*",
  "start-prod": "NODE_ENV=production node dist/server.js",
  "start-dev": "NODE_ENV=development node dist/server.js",
  "watch-dev": "nodemon --exec \"npm run build-dev && npm run start-dev\""
 },
 "devDependencies": {
  "autoprefixer": "^8.4.1",
  "babel-cli": "^6.26.0",
  "babel-core": "^6.26.3",
  "babel-loader": "^7.1.4",
  "babel-plugin-syntax-dynamic-import": "^6.18.0",
  "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
  "babel-plugin-transform-async-generator-functions": "^6.24.1",
  "babel-plugin-transform-class-properties": "^6.24.1",
  "babel-plugin-transform-export-extensions": "^6.22.0",
  "babel-plugin-transform-object-rest-spread": "^6.26.0",
  "babel-preset-env": "^1.6.1",
  "babel-preset-react": "^6.24.1",
  "copy-webpack-plugin": "^4.5.1",
  "extract-text-webpack-plugin": "^4.0.0-beta.0",
  "file-loader": "^1.1.11",
  "mini-css-extract-plugin": "^0.4.0",
  "nodemon": "^1.17.3",
  "npx": "^10.2.0",
  "react-svg-loader": "^2.1.0",
  "resolve-url-loader": "^2.3.0",
  "style-loader": "^0.21.0",
  "url-loader": "^1.0.1",
  "webpack": "^4.6.0",
  "webpack-cli": "^2.1.2",
  "webpack-dev-server": "^3.1.3"
 },
 "dependencies": {
  "axios": "^0.18.0",
  "cookie-session": "^2.0.0-beta.3",
  "cookies": "^0.7.1",
  "css-loader": "^0.28.11",
  "csvtojson": "^2.0.3",
  "dotenv": "^6.0.0",
  "express": "^4.16.3",
  "formidable": "^1.2.1",
  "gsap": "^1.20.4",
  "helmet": "^3.13.0",
  "history": "^4.7.2",
  "node-sass": "^4.9.0",
  "normalize.css": "^8.0.0",
  "postcss": "^6.0.22",
  "postcss-cssnext": "^3.1.0",
  "postcss-loader": "^2.1.4",
  "prop-types": "^15.6.1",
  "react": "^16.3.2",
  "react-context-component": "0.0.6",
  "react-csv-reader": "^0.3.2",
  "react-dom": "^16.3.2",
  "react-file-reader": "^1.1.4",
  "react-helmet": "^5.2.0",
  "react-redux": "^5.0.7",
  "react-router-dom": "^4.2.2",
  "react-router-redux": "^5.0.0-alpha.9",
  "redux": "^4.0.0",
  "rimraf": "^2.6.2",
  "sass-loader": "^7.0.1",
  "webpack-node-externals": "^1.7.2",
  "xlsx": "^0.13.3"
 }
}
